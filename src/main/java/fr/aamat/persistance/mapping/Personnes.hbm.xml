<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="fr.aamat.model.Personnes" table="personnes">
        <id name="id" type="int" column="id">
            <generator class="increment"/>
        </id>
        <property name="nomPersonne" type="string" not-null="true" />
        <property name="prenomPersonne" type="string" not-null="true" />
        <property name="datenaissPersonne" type="date"/>
        <many-to-one name="maison" unique="true" cascade="all" not-null="true"/>
    </class>
</hibernate-mapping>